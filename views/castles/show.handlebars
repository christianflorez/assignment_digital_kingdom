<h2>{{castle}}</h2>

<hr>
{{#each allLieges.Lieges as |liege| }}
<ul class="demo-list-icon mdl-list">
  <li class="mdl-list__item">
    <span class="mdl-list__item-primary-content">
    <i class="material-icons mdl-list__item-icon">face</i>
    Liege: <a href="/kingdoms/{{../kingdom}}/castles/{{../castle}}/lieges/{{@key}}"> {{@key}}</a><br>
    </span>
  </li>
  <li class="mdl-list__item">
    <span class="mdl-list__item-primary-content">
    <i class="material-icons mdl-list__item-icon">people</i>
    Vassal Count: {{liege.VassalCount}}<br>
    </span>
  </li>
  {{#each liege.Vassals}}
  <li class="mdl-list__item">
    <span class="mdl-list__item-primary-content">
    <i class="material-icons mdl-list__item-icon">person</i>
    Vassal: {{this}}
    </span>
  </li>
  {{/each}}
</ul>
<hr>
{{/each}}